a=int(input("Enter a;"))
b=int(input("Enter b;"))
c=int(input("Enter c;"))
if a > b:
    if a > c:
        print("a is largest")
else:
    print("c is largest")        
             
    
    
    
      
        


    
    
    

    
        



    
    
    
    

    
    

    
    
    
        
        
        


    